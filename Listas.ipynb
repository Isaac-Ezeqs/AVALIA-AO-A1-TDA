      "source": [
        "#Listas\n",
        "\n",
        "alunos = []\n",
        "\n",
        "qtd = int(input(\"Quantos alunos deseja cadastrar? \"))\n",
        "\n",
        "for i in range(qtd):\n",
        "    nome = input(f\"Digite o nome do aluno {i+1}: \")\n",
        "    alunos.append(nome)\n",
        "\n",
        "print(\"\\nLista de alunos cadastrados:\")\n",
        "for nome in alunos:\n",
        "    print(\"-\", nome)"
      ]
    }
  ]
