      "source": [
        "#Estruturas de Repetição\n",
        "\n",
        "print(\"Números pares de 1 a 100 usando for:\")\n",
        "for i in range(1, 101):\n",
        "    if i % 2 == 0:\n",
        "        print(i, end=\" \")\n",
        "\n",
        "print(\"\\n\\nNúmeros pares de 1 a 100 usando while:\")\n",
        "i = 1\n",
        "while i <= 100:\n",
        "    if i % 2 == 0:\n",
        "        print(i, end=\" \")\n",
        "    i += 1"
      ]
    }
  ]
